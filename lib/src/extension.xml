<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.batch.ane</id>
    <versionNumber>1.3.2</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>androidane.jar</nativeLibrary>
				<initializer>com.batch.ane.android.Extension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86"> 
        	<applicationDeployment>  
				<nativeLibrary>androidane.jar</nativeLibrary>  
				<initializer>com.batch.ane.android.Extension</initializer>
			</applicationDeployment>  
		</platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libane-ios.a</nativeLibrary>
                <initializer>BatchExtensionInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
	</platforms>
</extension>
